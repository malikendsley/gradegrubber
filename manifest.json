{
    "manifest_version": 3,
    "name": "Grade Grubber",
    "description": "Simple CourseWorks Analytics",
    "version": "1.0",
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "injection.js"
            ]
        }
    ],
    "action": {
        "default_icon": "test_image.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage"
    ]
}