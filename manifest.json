{
  "manifest_version": 3,
  "name": "Grade Grubber",
  "permissions": ["storage", "notifications"],
  "description": "Simple CourseWorks Analytics",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "action": {
    "default_icon": "test_image.png"
  }
}