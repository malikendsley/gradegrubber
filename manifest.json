{
    "manifest_version": 2,
    "name": "Grade Grubber",
    "description": "Simple CourseWorks Analytics",
    "version": "1.0",
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["gg-injection.js"],
            "css": ["gg-injection-style.css"]
            
        }
    ],
    "browser_action": {
        "default_icon": "gg.png",
        "default_popup": "settings-popup.html"
    },
    "permissions": [
        "storage"
    ],
    "icons": {
        "16": "gg.png",
        "48": "gg.png",
        "128": "gg.png"
    }, 
    "web_accessible_resources": [
        "gg.png"
      ]
}