{
    "manifest_version": 3,
    "name": "Grade Grubber",
    "description": "Simple CourseWorks Analytics",
    "version": "1.0",
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": "gg.png",
        "default_popup": "settings-popup.html"
    },
    "permissions": [
        "storage"
    ],
    "icons": {
        "16": "gg.png",
        "48": "gg.png",
        "128": "gg.png"
    },
    "web_accessible_resources": [
        {
          "resources": ["gg.png"],
          "matches": ["<all_urls>"]
        }
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["gg-injection.js"]
        }
    ]
}