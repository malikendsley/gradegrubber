{
    "manifest_version": 2,
    "name": "Grade Grubber",
    "description": "Simple CourseWorks Analytics",
    "version": "1.0",
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["gg-injection.js"]
        }
    ],
    "browser_action": {
        "default_icon": "test_image.png",
        "default_popup": "settings-popup.html"
    },
    "permissions": [
        "storage"
    ]
}