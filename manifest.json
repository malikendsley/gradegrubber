{
    "manifest_version": 3,
    "name": "Grade Grubber",
    "description": "Grade Grubber is an enhances the Courseworks Grades page with key stats, color coding, and customization! Stores no data.",
    "version": "0.1",
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": "gg.png",
        "default_popup": "settings-popup.html"
    },
    "permissions": [
        "storage"
    ],
    "icons": {
        "16": "gg16.png",
        "48": "gg48.png",
        "128": "gg.png"
    },
    "web_accessible_resources": [
        {
          "resources": ["gg.png"],
          "matches": ["<all_urls>"]
        }
    ],
    "content_scripts": [
        {
            "matches": ["https://courseworks2.columbia.edu/*"],
            "js": ["gg-injection.js"]
        }
    ]
}